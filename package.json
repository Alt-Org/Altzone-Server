    "@nestjs/platform-express": "11.1.3",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/swagger": "11.2.0",
    "argon2": "0.43.0",
    "bull": "4.16.5",
    "bullmq": "5.54.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cookie-parser": "1.4.7",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "express-basic-auth": "1.2.1",
    "ioredis": "5.6.1",
    "lodash": "4.17.21",
    "mongodb": "6.16.0",
